<div id="<%= dom_id medico %>">
  <p>
    <strong>Nombre:</strong>
    <%= medico.nombre %>
  </p>

  <p>
    <strong>Apellido:</strong>
    <%= medico.apellido %>
  </p>

  <p>
    <strong>Especialidad:</strong>
    <%= medico.especialidad %>
  </p>

  <p>
    <strong>Telefono:</strong>
    <%= medico.telefono %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= medico.email %>
